        "GroupName": "admin",
        "Precedence": 2,
        "RoleArn": {
          "Fn::GetAtt": [
            "adminGroupRole",
            "Arn"
          ]
        }
      }
    },
    "adminGroupRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Sid": "",
              "Effect": "Allow",
              "Principal": {
                "Federated": "cognito-identity.amazonaws.com"
              },
              "Action": "sts:AssumeRoleWithWebIdentity",
              "Condition": {
                "StringEquals": {
                  "cognito-identity.amazonaws.com:aud": {
                    "Ref": "authadsatsFlutterAuthIdentityPoolId"
                  }
                },
                "ForAnyValue:StringLike": {
                  "cognito-identity.amazonaws.com:amr": "authenticated"
                }
              }
            }
          ]
        },
        "RoleName": {
          "Fn::Join": [
            "",
            [
              {
                "Ref": "authadsatsFlutterAuthUserPoolId"
              },
              "-adminGroupRole"
            ]
          ]
        }
      }
    },
    "crewGroup": {
      "Type": "AWS::Cognito::UserPoolGroup",
      "Properties": {
        "UserPoolId": {
          "Ref": "authadsatsFlutterAuthUserPoolId"
        },
        "Description": "override success",
        "GroupName": "crew",
        "Precedence": 3,
        "RoleArn": {
          "Fn::GetAtt": [
            "crewGroupRole",
            "Arn"
          ]
        }
      }
    },
    "crewGroupRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Sid": "",
              "Effect": "Allow",
              "Principal": {
                "Federated": "cognito-identity.amazonaws.com"
              },
              "Action": "sts:AssumeRoleWithWebIdentity",
              "Condition": {
                "StringEquals": {
                  "cognito-identity.amazonaws.com:aud": {
                    "Ref": "authadsatsFlutterAuthIdentityPoolId"
                  }
                },
                "ForAnyValue:StringLike": {
                  "cognito-identity.amazonaws.com:amr": "authenticated"
                }
              }
            }
          ]
        },
        "RoleName": {
          "Fn::Join": [
            "",
            [
              {
                "Ref": "authadsatsFlutterAuthUserPoolId"
              },
              "-crewGroupRole"
            ]
          ]
        }
      }
    }
  },
  "Outputs": {
